@article{polaz_2009,
	abstract = {Uma forma de operacionalizar o conceito de sustentabilidade {\'{e}} por meio da utiliza{\c{c}}{\~{a}}o de indicadores, um instrumento que pode auxiliar os gestores p{\'{u}}blicos no processo decis{\'{o}}rio. Este trabalho teve como objetivo a proposi{\c{c}}{\~{a}}o de indicadores de sustentabilidade para a gest{\~{a}}o de Res{\'{i}}duos S{\'{o}}lidos Urbanos (RSU) em S{\~{a}}o Carlos (SP), sob a perspectiva de cinco dimens{\~{o}}es: ambiental, econ{\^{o}}mica, social, pol{\'{i}}tica e cultural. A estrat{\'{e}}gia adotada para a elabora{\c{c}}{\~{a}}o dos indicadores foi a identifica{\c{c}}{\~{a}}o dos problemas priorit{\'{a}}rios para a gest{\~{a}}o de RSU por meio de consultas aos gestores municipais. Isto resultou em um conjunto de 15 indicadores, cujos valores expressam sua tend{\^{e}}ncia favor{\'{a}}vel, desfavor{\'{a}}vel ou muito desfavor{\'{a}}vel {\`{a}} sustentabilidade. Esta experi{\^{e}}ncia mostrou que a implementa{\c{c}}{\~{a}}o de um sistema de indicadores deve envolver a participa{\c{c}}{\~{a}}o de diferentes agentes sociais, a fim de legitimar o uso efetivo desta importante ferramenta.One way to operationalize the concept of sustainability is through the use of indicators, an important tool to guide the public managers in the process of making decisions. The objective of this paper was the proposition of sustainability indicators for public management of Municipal Solid Waste (MSW) in S{\~{a}}o Carlos (SP), from the perspective of five dimensions: environmental, economic, social, political and cultural. The strategy adopted for the indicators development was the identification of priority problems for the MSW management, by interviewing the municipal managers. The results include a set of 15 sustainability indicators, as their values express three trends: favorable, unfavorable or very unfavorable to sustainability. This research showed that the development of a local indicator system should involve different actors in order to legitimize the effective use of this useful management tool.},
	author = {Polaz, Carla Natacha Marcolino and Teixeira, Bernardo Arantes do Nascimento},
	file = {:C$\backslash$:/Users/liks9/Downloads/refs{\_}finais/POLAZ e TEIXEIRA.pdf:pdf},
	issn = {14134152},
	journal = {Engenharia Sanitaria e Ambiental},
	keywords = {Environmental management,Municipal solid waste,Public policy,Sustainability indicators},
	number = {3},
	pages = {411--420},
	title = {{Indicadores de sustentabilidade para a gest{\~{a}}o municipal de res{\'{i}}duos s{\'{o}}lidos urbanos: Um estudo para S{\~{a}}o Carlos (SP)}},
	volume = {14},
	year = {2009}
}
