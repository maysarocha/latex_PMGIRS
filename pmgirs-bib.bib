school = {Universidade de S{\~{a}}o Paulo - Escola de Engenharia de S{\~{a}}o Carlos},
title = {{Res{\'{i}}duos da colheita e beneficiamento da caixeta - Tabebuia cassionides (Lam.) DC.: caracteriza{\c{c}}{\~{a}}o e perpectivas}},
url = {http://dedalus.usp.br/F/PG12FFDFBDMSE5KTIUPAL2EN3QUVRPLD9TQKX9NVV7BBNEQ84H-02957?func=direct{\&}doc{\_}number=001098472{\&}pds{\_}handle=GUEST https://bdpi.usp.br/item/001098472},
year = {2000}
