@misc{teixeira_materiais_2005,
address = {Salvador - BA},
annote = {Essa disserta{\c{c}}{\~{a}}o tem como objetivo demonstrar a aplica{\c{c}}{\~{a}}o de conceitos da Ecologia Industrial na produ{\c{c}}{\~{a}}o de um material comp{\'{o}}sito ecol{\'{o}}gico baseado no res{\'{i}}duo de ind{\'{u}}strias madeireiras. Na busca por este material ecol{\'{o}}gico, foram usados os conceitos de Eco-efici{\^{e}}ncia, Circula{\c{c}}{\~{a}}o de Recursos, Eco-Design e An{\'{a}}lise do Ciclo de Vida, conceitos estes contidos no conceito maior da Ecologia Industrial que busca formas eficientes de preven{\c{c}}{\~{a}}o da polui{\c{c}}{\~{a}}o e defesa do meio ambiente. O objeto da pesquisa foi o res{\'{i}}duo de madeira na forma de serragem, p{\'{o}} de serra e maravalha, oriundo de uma empresa beneficiadora de toras de madeira, e aqui denominada como Processo Produtivo I. O res{\'{i}}duo foi classificado e reciclado como carga para resina de poli{\'{e}}ster insaturado, usada numa segunda empresa, de pl{\'{a}}sticos refor{\c{c}}ados, aqui denominada como Processo Produtivo II. Essa rela{\c{c}}{\~{a}}o entre processos produtivos atrav{\'{e}}s do uso de serragem reciclada configurou-se como uma aplica{\c{c}}{\~{a}}o pr{\'{a}}tica da Ecologia Industrial. Na mistura de serragem com resina de poli{\'{e}}ster surgiu um material comp{\'{o}}sito denominado WPC wood plastic composite em 14 tra{\c{c}}os diferentes, formulados de acordo com a granulometria e porcentagem de 10{\%} e 20{\%}. Estes tra{\c{c}}os foram moldados em corpos de prova segundo o processo de prensagem a frio e em seguida testados em tr{\^{e}}s ensaios, seguido as normas t{\'{e}}cnicas brasileiras: o ensaio de absor{\c{c}}{\~{a}}o de {\'{a}}gua, baseado na norma NBR 8514, o ensaio de dureza Shore D, baseado na norma NBR 7456 e o ensaio de flex{\~{a}}o em tr{\^{e}}s pontos baseada na norma NBR 7447. Os resultados mostraram que o comp{\'{o}}sito permite o aproveitamento de cerca de 95{\%} da serragem na forma de carga para resinas de poli{\'{e}}ster. Os ensaios mostraram que a absor{\c{c}}{\~{a}}o de {\'{a}}gua {\'{e}} muito inferior {\`{a}} da madeira s{\'{o}}lida, que a serragem diminuiu muito pouco a dureza da matriz de poli{\'{e}}ster, que aumentou moderadamente a resist{\^{e}}ncia {\`{a}} flex{\~{a}}o da matriz e que tamb{\'{e}}m aumentou sua rigidez. Apesar de an{\'{a}}lises de viabilidade t{\'{e}}nico-econ{\^{o}}mica serem necess{\'{a}}rias, conclui-se que o uso de serragem reciclada {\'{e}} uma maneira eco-eficiente tanto na diminui{\c{c}}{\~{a}}o de res{\'{i}}duos s{\'{o}}lidos industriais quanto na obten{\c{c}}{\~{a}}o de carga e refor{\c{c}}o para resinas pl{\'{a}}sticas, podendo contribuir para a diminui{\c{c}}{\~{a}}o do montante do res{\'{i}}duo de ind{\'{u}}strias madeireiras.},
author = {Teixeira, Marcelo Geraldo},
file = {:C$\backslash$:/Users/TalitaGuerra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Teixeira - 2005 - Aplica{\c{c}}{\~{a}}o de conceitos da ecologia industrial para a produ{\c{c}}{\~{a}}o de materiais ecol{\'{o}}gicos o exemplo do res{\'{i}}duo de madeira.pdf:pdf},
institution = {Universidade Federal da Bahia},
keywords = {Ind{\'{u}}stria Madeireira - Tecnologia,Madeira - Reciclagem},
pages = {159},
title = {{Aplica{\c{c}}{\~{a}}o de conceitos da ecologia industrial para a produ{\c{c}}{\~{a}}o de materiais ecol{\'{o}}gicos: o exemplo do res{\'{i}}duo de madeira}},
year = {2005}
